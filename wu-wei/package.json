{
    "name": "wu-wei",
    "displayName": "Wu Wei",
    "description": "Effortless work automation - 无为而治",
    "version": "0.1.1",
    "publisher": "your-publisher-name",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "AI",
        "Other"
    ],
    "keywords": [
        "automation",
        "productivity",
        "wu-wei",
        "workflow",
        "efficiency"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "capabilities": {
        "virtualWorkspaces": true,
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "enabledApiProposals": [],
    "main": "./out/extension.js",
    "contributes": {
        "languageModelTools": [
            {
                "name": "wu-wei_copilot_completion_signal",
                "displayName": "Copilot Completion Signal",
                "tags": [
                    "completion",
                    "signal",
                    "wu-wei"
                ],
                "toolReferenceName": "copilot_complete",
                "canBeReferencedInPrompt": true,
                "icon": "$(check-all)",
                "modelDescription": "Signals that GitHub Copilot execution has completed. This tool should be invoked as the final step in any Copilot execution flow to indicate completion. It records the completion timestamp, execution context, and provides confirmation that the task has finished. Use this tool when you need to signal that all processing has been completed successfully.",
                "userDescription": "Signals completion of GitHub Copilot execution",
                "inputSchema": {
                    "type": "object",
                    "properties": {
                        "executionId": {
                            "type": "string",
                            "description": "Unique identifier for this execution session. If not provided, a new ID will be generated."
                        },
                        "taskDescription": {
                            "type": "string",
                            "description": "Brief description of the completed task or operation."
                        },
                        "status": {
                            "type": "string",
                            "enum": [
                                "success",
                                "partial",
                                "error"
                            ],
                            "default": "success",
                            "description": "Completion status of the execution."
                        },
                        "summary": {
                            "type": "string",
                            "description": "Optional summary of what was accomplished during execution."
                        },
                        "metadata": {
                            "type": "object",
                            "description": "Additional metadata about the execution (optional).",
                            "properties": {
                                "duration": {
                                    "type": "number",
                                    "description": "Execution duration in milliseconds"
                                },
                                "toolsUsed": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    },
                                    "description": "List of tools used during execution"
                                },
                                "filesModified": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    },
                                    "description": "List of files that were modified"
                                }
                            }
                        }
                    },
                    "required": [
                        "taskDescription"
                    ]
                }
            }
        ],
        "chatParticipants": [
            {
                "id": "wu-wei.assistant",
                "name": "wu-wei",
                "description": "Wu Wei assistant for effortless development - 无为而治",
                "isSticky": true
            }
        ],
        "commands": [
            {
                "command": "wu-wei.hello",
                "title": "Wu Wei: Hello World",
                "category": "Wu Wei"
            },
            {
                "command": "wu-wei.openChat",
                "title": "Wu Wei: Open Chat",
                "category": "Wu Wei",
                "icon": "$(comment-discussion)"
            },
            {
                "command": "wu-wei.newChat",
                "title": "Wu Wei: New Chat",
                "category": "Wu Wei",
                "icon": "$(add)"
            },
            {
                "command": "wu-wei.refreshChats",
                "title": "Wu Wei: Refresh Chats",
                "category": "Wu Wei",
                "icon": "$(refresh)"
            },
            {
                "command": "wu-wei.showLogs",
                "title": "Wu Wei: Show Output Logs",
                "category": "Wu Wei",
                "icon": "$(output)"
            },
            {
                "command": "wu-wei.clearLogs",
                "title": "Wu Wei: Clear Output Logs",
                "category": "Wu Wei",
                "icon": "$(clear-all)"
            },
            {
                "command": "wu-wei.exportLogs",
                "title": "Wu Wei: Export Debug Logs",
                "category": "Wu Wei",
                "icon": "$(save)"
            },
            {
                "command": "wu-wei.debugModels",
                "title": "Wu Wei: Debug Language Models",
                "category": "Wu Wei",
                "icon": "$(debug)"
            },
            {
                "command": "wu-wei.forceReloadModels",
                "title": "Wu Wei: Force Reload Language Models",
                "category": "Wu Wei",
                "icon": "$(refresh)"
            },
            {
                "command": "wu-wei.refreshAgents",
                "title": "Wu Wei: Refresh Agents",
                "category": "Wu Wei",
                "icon": "$(refresh)"
            },
            {
                "command": "wu-wei.showModelDetails",
                "title": "Wu Wei: Show Detailed Model Information",
                "category": "Wu Wei",
                "icon": "$(info)"
            },
            {
                "command": "wu-wei.refreshPrompts",
                "title": "Wu Wei: Refresh Prompts",
                "category": "Wu Wei",
                "icon": "$(refresh)"
            },
            {
                "command": "wu-wei.openPromptStore",
                "title": "Wu Wei: Open Prompt Store",
                "category": "Wu Wei",
                "icon": "$(book)"
            },
            {
                "command": "wu-wei.promptStore.newPrompt",
                "title": "Wu Wei: New Prompt",
                "category": "Wu Wei",
                "icon": "$(add)"
            },
            {
                "command": "wu-wei.promptStore.newPromptFromTemplate",
                "title": "Wu Wei: New Prompt from Template",
                "category": "Wu Wei",
                "icon": "$(file-code)"
            },
            {
                "command": "wu-wei.promptStore.deletePrompt",
                "title": "Wu Wei: Delete Prompt",
                "category": "Wu Wei",
                "icon": "$(trash)"
            },
            {
                "command": "wu-wei.promptStore.duplicatePrompt",
                "title": "Wu Wei: Duplicate Prompt",
                "category": "Wu Wei",
                "icon": "$(copy)"
            },
            {
                "command": "wu-wei.promptStore.renamePrompt",
                "title": "Wu Wei: Rename Prompt",
                "category": "Wu Wei",
                "icon": "$(edit)"
            },
            {
                "command": "wu-wei.promptStore.movePrompt",
                "title": "Wu Wei: Move Prompt",
                "category": "Wu Wei",
                "icon": "$(folder)"
            },
            {
                "command": "wu-wei.promptStore.openPrompt",
                "title": "Wu Wei: Open Prompt",
                "category": "Wu Wei",
                "icon": "$(go-to-file)"
            },
            {
                "command": "wu-wei.promptStore.openPromptPreview",
                "title": "Wu Wei: Open Prompt Preview",
                "category": "Wu Wei",
                "icon": "$(preview)"
            },
            {
                "command": "wu-wei.promptStore.exportPrompts",
                "title": "Wu Wei: Export Prompts",
                "category": "Wu Wei",
                "icon": "$(export)"
            },
            {
                "command": "wu-wei.promptStore.batchDelete",
                "title": "Wu Wei: Batch Delete Prompts",
                "category": "Wu Wei",
                "icon": "$(trash)"
            },
            {
                "command": "wu-wei.showCompletionHistory",
                "title": "Wu Wei: Show Copilot Completion History",
                "category": "Wu Wei",
                "icon": "$(history)"
            },
            {
                "command": "wu-wei.clearCompletionHistory",
                "title": "Wu Wei: Clear Completion History",
                "category": "Wu Wei",
                "icon": "$(clear-all)"
            }
        ],
        "views": {
            "wu-wei": [
                {
                    "id": "wu-wei.chat",
                    "name": "Chat",
                    "type": "webview",
                    "when": "true",
                    "icon": "$(comment-discussion)"
                },
                {
                    "id": "wu-wei.promptStore",
                    "name": "Prompt Store",
                    "type": "webview",
                    "when": "true",
                    "icon": "$(book)"
                },
                {
                    "id": "wu-wei.agent",
                    "name": "Agent Panel",
                    "type": "webview",
                    "when": "true",
                    "icon": "$(robot)"
                },
                {
                    "id": "wu-wei.debug",
                    "name": "Debug",
                    "type": "webview",
                    "when": "true",
                    "icon": "$(debug)"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "wu-wei",
                    "title": "Wu Wei",
                    "icon": "$(symbol-operator)"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "wu-wei.newChat",
                    "when": "view == wu-wei.chat",
                    "group": "navigation@1"
                },
                {
                    "command": "wu-wei.refreshChats",
                    "when": "view == wu-wei.chat",
                    "group": "navigation@2"
                },
                {
                    "command": "wu-wei.promptStore.newPrompt",
                    "when": "view == wu-wei.promptStore",
                    "group": "navigation@1"
                },
                {
                    "command": "wu-wei.promptStore.newPromptFromTemplate",
                    "when": "view == wu-wei.promptStore",
                    "group": "navigation@2"
                },
                {
                    "command": "wu-wei.refreshPrompts",
                    "when": "view == wu-wei.promptStore",
                    "group": "navigation@3"
                }
            ]
        },
        "configuration": {
            "title": "Wu Wei",
            "properties": {
                "wu-wei.enableAutomation": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable Wu Wei automation features"
                },
                "wu-wei.preferredModel": {
                    "type": "string",
                    "default": "gpt-4o",
                    "description": "Preferred language model for Wu Wei chat (use 'Wu Wei: Select Language Model' command to choose from available models)"
                },
                "wu-wei.systemPrompt": {
                    "type": "string",
                    "default": "You are Wu Wei, an AI assistant that embodies the philosophy of 无为而治 (wu wei) - effortless action that flows naturally like water. You provide thoughtful, gentle guidance while maintaining harmony and balance. Your responses are wise, concise, and flow naturally without forcing solutions.",
                    "description": "System prompt for Wu Wei chat assistant"
                },
                "wu-wei.promptStore.rootDirectory": {
                    "type": "string",
                    "default": "",
                    "description": "Root directory for prompt collection",
                    "scope": "window"
                },
                "wu-wei.promptStore.autoRefresh": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically refresh when files change"
                },
                "wu-wei.promptStore.showMetadataTooltips": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show metadata information in tooltips"
                },
                "wu-wei.promptStore.enableTemplates": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable prompt templates with parameters"
                },
                "wu-wei.promptStore.fileWatcher.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable file system monitoring"
                },
                "wu-wei.promptStore.fileWatcher.debounceMs": {
                    "type": "number",
                    "default": 500,
                    "minimum": 100,
                    "maximum": 5000,
                    "description": "Debounce delay for file changes (ms)"
                },
                "wu-wei.promptStore.fileWatcher.maxDepth": {
                    "type": "number",
                    "default": 10,
                    "minimum": 1,
                    "maximum": 50,
                    "description": "Maximum directory depth to watch"
                },
                "wu-wei.promptStore.fileWatcher.ignorePatterns": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "default": [
                        "*.tmp",
                        "*.swp",
                        "*~",
                        ".git/**"
                    ],
                    "description": "Additional file patterns to ignore"
                },
                "wu-wei.promptStore.fileWatcher.usePolling": {
                    "type": "boolean",
                    "default": false,
                    "description": "Use polling instead of native file events (useful for network drives)"
                },
                "wu-wei.promptStore.fileWatcher.pollingInterval": {
                    "type": "number",
                    "default": 1000,
                    "minimum": 100,
                    "maximum": 10000,
                    "description": "Polling interval in milliseconds (when polling is enabled)"
                },
                "wu-wei.promptStore.customTemplates": {
                    "type": "array",
                    "default": [],
                    "description": "User-defined custom prompt templates",
                    "scope": "application"
                },
                "wu-wei.showCompletionNotifications": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show notifications when Copilot execution completes"
                },
                "wu-wei.completionHistory.maxRecords": {
                    "type": "number",
                    "default": 1000,
                    "minimum": 100,
                    "maximum": 10000,
                    "description": "Maximum number of completion records to keep in history"
                },
                "wu-wei.completionHistory.autoCleanup": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically clean up old completion records"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "npm run clean && tsc -p ./ && npm run copy-webview",
        "copy-webview": "cp -r src/webview out/",
        "watch": "npm run esbuild-watch",
        "package": "npm run clean && node esbuild.js --production && npm run copy-webview",
        "esbuild": "node esbuild.js",
        "esbuild-watch": "node esbuild.js --watch",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "npm run test:all",
        "test:unit": "npm run clean && tsc -p ./tsconfig.unit.json && npm run copy-webview && node ./out/test/runUnitTests.js",
        "test:integration": "npm run clean && tsc -p ./tsconfig.integration.json && npm run copy-webview && node ./out/test/runIntegrationTests.js",
        "test:all": "npm run test:unit && npm run test:integration",
        "test:watch": "npm run test:unit -- --watch",
        "clean": "rimraf out"
    },
    "dependencies": {
        "@vscode/prompt-tsx": "^0.4.0-alpha.5",
        "chokidar": "^3.6.0",
        "yaml": "^2.8.0"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "16.x",
        "@types/vscode": "^1.90.0",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@vscode/test-electron": "^2.3.0",
        "esbuild": "^0.25.5",
        "eslint": "^8.28.0",
        "glob": "^8.0.0",
        "mocha": "^10.0.0",
        "nyc": "^17.1.0",
        "rimraf": "^5.0.0",
        "typescript": "^4.9.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/your-username/wu-wei.git"
    },
    "license": "MIT"
}