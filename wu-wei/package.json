{
    "name": "wu-wei",
    "displayName": "Wu Wei",
    "description": "Effortless work automation - 无为而治",
    "version": "0.1.0",
    "publisher": "your-publisher-name",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "Productivity",
        "Other"
    ],
    "keywords": [
        "automation",
        "productivity",
        "wu-wei",
        "workflow",
        "efficiency"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "capabilities": {
        "virtualWorkspaces": true,
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "enabledApiProposals": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "wu-wei.hello",
                "title": "Wu Wei: Hello World",
                "category": "Wu Wei"
            },
            {
                "command": "wu-wei.openChat",
                "title": "Wu Wei: Open Chat",
                "category": "Wu Wei",
                "icon": "$(comment-discussion)"
            },
            {
                "command": "wu-wei.newChat",
                "title": "Wu Wei: New Chat",
                "category": "Wu Wei",
                "icon": "$(add)"
            },
            {
                "command": "wu-wei.refreshChats",
                "title": "Wu Wei: Refresh Chats",
                "category": "Wu Wei",
                "icon": "$(refresh)"
            },
            {
                "command": "wu-wei.showLogs",
                "title": "Wu Wei: Show Output Logs",
                "category": "Wu Wei",
                "icon": "$(output)"
            },
            {
                "command": "wu-wei.clearLogs",
                "title": "Wu Wei: Clear Output Logs",
                "category": "Wu Wei",
                "icon": "$(clear-all)"
            },
            {
                "command": "wu-wei.exportLogs",
                "title": "Wu Wei: Export Debug Logs",
                "category": "Wu Wei",
                "icon": "$(save)"
            },
            {
                "command": "wu-wei.debugModels",
                "title": "Wu Wei: Debug Language Models",
                "category": "Wu Wei",
                "icon": "$(debug)"
            },
            {
                "command": "wu-wei.forceReloadModels",
                "title": "Wu Wei: Force Reload Language Models",
                "category": "Wu Wei",
                "icon": "$(refresh)"
            },
            {
                "command": "wu-wei.sendAgentRequest",
                "title": "Wu Wei: Send Agent Request",
                "category": "Wu Wei",
                "icon": "$(send)"
            },
            {
                "command": "wu-wei.refreshAgents",
                "title": "Wu Wei: Refresh Agents",
                "category": "Wu Wei",
                "icon": "$(refresh)"
            },
            {
                "command": "wu-wei.showModelDetails",
                "title": "Wu Wei: Show Detailed Model Information",
                "category": "Wu Wei",
                "icon": "$(info)"
            },
            {
                "command": "wu-wei.refreshPrompts",
                "title": "Wu Wei: Refresh Prompts",
                "category": "Wu Wei",
                "icon": "$(refresh)"
            },
            {
                "command": "wu-wei.openPromptStore",
                "title": "Wu Wei: Open Prompt Store",
                "category": "Wu Wei",
                "icon": "$(book)"
            },
            {
                "command": "wu-wei.promptStore.selectDirectory",
                "title": "Wu Wei: Select Prompt Store Directory",
                "category": "Wu Wei",
                "icon": "$(folder-opened)"
            },
            {
                "command": "wu-wei.promptStore.newPrompt",
                "title": "Wu Wei: New Prompt",
                "category": "Wu Wei",
                "icon": "$(add)"
            },
            {
                "command": "wu-wei.promptStore.refreshStore",
                "title": "Wu Wei: Refresh Prompt Store",
                "category": "Wu Wei",
                "icon": "$(refresh)"
            }
        ],
        "views": {
            "wu-wei": [
                {
                    "id": "wu-wei.chat",
                    "name": "Chat",
                    "type": "webview",
                    "when": "true",
                    "icon": "$(comment-discussion)"
                },
                {
                    "id": "wu-wei.promptStore",
                    "name": "Prompt Store",
                    "type": "webview",
                    "when": "true",
                    "icon": "$(book)"
                },
                {
                    "id": "wu-wei.agent",
                    "name": "Agent Panel",
                    "type": "webview",
                    "when": "true",
                    "icon": "$(robot)"
                },
                {
                    "id": "wu-wei.debug",
                    "name": "Debug",
                    "type": "webview",
                    "when": "true",
                    "icon": "$(debug)"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "wu-wei",
                    "title": "Wu Wei",
                    "icon": "$(symbol-operator)"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "wu-wei.newChat",
                    "when": "view == wu-wei.chat",
                    "group": "navigation@1"
                },
                {
                    "command": "wu-wei.refreshChats",
                    "when": "view == wu-wei.chat",
                    "group": "navigation@2"
                },
                {
                    "command": "wu-wei.refreshPrompts",
                    "when": "view == wu-wei.promptStore",
                    "group": "navigation@1"
                },
                {
                    "command": "wu-wei.promptStore.selectDirectory",
                    "when": "view == wu-wei.promptStore",
                    "group": "navigation@2"
                },
                {
                    "command": "wu-wei.promptStore.newPrompt",
                    "when": "view == wu-wei.promptStore",
                    "group": "navigation@3"
                }
            ]
        },
        "configuration": {
            "title": "Wu Wei",
            "properties": {
                "wu-wei.enableAutomation": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable Wu Wei automation features"
                },
                "wu-wei.preferredModel": {
                    "type": "string",
                    "default": "gpt-4o",
                    "description": "Preferred language model for Wu Wei chat (use 'Wu Wei: Select Language Model' command to choose from available models)"
                },
                "wu-wei.systemPrompt": {
                    "type": "string",
                    "default": "You are Wu Wei, an AI assistant that embodies the philosophy of 无为而治 (wu wei) - effortless action that flows naturally like water. You provide thoughtful, gentle guidance while maintaining harmony and balance. Your responses are wise, concise, and flow naturally without forcing solutions.",
                    "description": "System prompt for Wu Wei chat assistant"
                },
                "wu-wei.promptStore.rootDirectory": {
                    "type": "string",
                    "default": "",
                    "description": "Root directory for prompt collection",
                    "scope": "window"
                },
                "wu-wei.promptStore.autoRefresh": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically refresh when files change"
                },
                "wu-wei.promptStore.showMetadataTooltips": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show metadata information in tooltips"
                },
                "wu-wei.promptStore.enableTemplates": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable prompt templates with parameters"
                },
                "wu-wei.promptStore.fileWatcher.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable file system monitoring"
                },
                "wu-wei.promptStore.fileWatcher.debounceMs": {
                    "type": "number",
                    "default": 500,
                    "minimum": 100,
                    "maximum": 5000,
                    "description": "Debounce delay for file changes (ms)"
                },
                "wu-wei.promptStore.fileWatcher.maxDepth": {
                    "type": "number",
                    "default": 10,
                    "minimum": 1,
                    "maximum": 50,
                    "description": "Maximum directory depth to watch"
                },
                "wu-wei.promptStore.fileWatcher.ignorePatterns": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "default": [
                        "*.tmp",
                        "*.swp",
                        "*~",
                        ".git/**"
                    ],
                    "description": "Additional file patterns to ignore"
                },
                "wu-wei.promptStore.fileWatcher.usePolling": {
                    "type": "boolean",
                    "default": false,
                    "description": "Use polling instead of native file events (useful for network drives)"
                },
                "wu-wei.promptStore.fileWatcher.pollingInterval": {
                    "type": "number",
                    "default": 1000,
                    "minimum": 100,
                    "maximum": 10000,
                    "description": "Polling interval in milliseconds (when polling is enabled)"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "test:watch": "npm run test -- --watch",
        "clean": "rimraf out"
    },
    "dependencies": {
        "chokidar": "^3.6.0",
        "yaml": "^2.8.0"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "16.x",
        "@types/vscode": "^1.90.0",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@vscode/test-electron": "^2.3.0",
        "eslint": "^8.28.0",
        "glob": "^8.0.0",
        "mocha": "^10.0.0",
        "rimraf": "^5.0.0",
        "typescript": "^4.9.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/your-username/wu-wei.git"
    },
    "license": "MIT"
}